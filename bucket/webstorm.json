{
    "version": "2025.1.2",
    "description": "JetBrains 的 IDE 产品，用于 Web 开发 (JavaScript，TypeScript)。A IDE by JetBrains for web development (JavaScript, TypeScript).",
    "homepage": "https://www.jetbrains.com/webstorm/",
    "license": {
        "identifier": "Unknown",
        "url": "https://www.jetbrains.com/store/license.html"
    },
    "architecture": {
        "64bit": {
            "url": "https://download.jetbrains.com/webstorm/WebStorm-2025.1.2.exe",
            "hash": "4981e5b54ac810a54036cad37762108cb46a08d94662967f17dfdc858d9a4dde"
        },
        "arm64": {
            "url": "https://download.jetbrains.com/webstorm/WebStorm-2025.1.2-aarch64.exe",
            "hash": "654c96ecc69e7f1dcc83d678e43c50d6614fa45180a13a8ed11c07504b8d6c17"
        }
    },
    "bin": [
        [
            "App\\bin\\webstorm64.exe",
            "WebStorm"
        ]
    ],
    "shortcuts": [
        [
            "App\\bin\\webstorm64.exe",
            "WebStorm 2025.1.2"
        ]
    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "$LogLocation = '\"' + \"$env:TEMP\\scoop-$app-logs\" + '\"'",
        "A-Install-Exe -ArgumentList @('/S', \"/LOG=$LogLocation\", \"/D=$dir\\App\") -SuccessFile 'App\\bin\\Uninstall.exe'"
    ],
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"WebStorm$versionShort\"",
        "$LinkPaths = @(\"$env:AppData\\JetBrains\\$DataDir\")",
        "$LinkTargets = @(\"$persist_dir\\JetBrains\\$DataDir\")",
        "A-New-LinkDirectory $LinkPaths $LinkTargets"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Stop-Process",
        "A-Uninstall-Exe -Uninstaller 'App\\bin\\Uninstall.exe' -ArgumentList @('/S')",
        "A-Remove-Link",
        "$versionPart = $version -split '\\.'",
        "$versionShort = \"$($versionPart[0]).$($versionPart[1])\"",
        "$DataDir = \"WebStorm$versionShort\"",
        "A-Remove-TempData @(\"$env:LocalAppData\\JetBrains\\$DataDir\")"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'JetBrains.WebStorm' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_user.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_user.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_user.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        },
        "shortcuts": [
            [
                "App\\bin\\webstorm64.exe",
                "WebStorm $version"
            ]
        ]
    }
}
