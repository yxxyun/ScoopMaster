{
    "version": "9.2.2",
    "description": "Visual Novel Translator, support HOOK / OCR /clipboard",
    "homepage": "https://github.com/HIllya51/LunaTranslator",
    "license": "GPL-3.0-only",
    "url": "https://github.com/HIllya51/LunaTranslator/releases/download/v9.2.2/LunaTranslator.zip",
    "hash": "2acf4bc06a284fcc20659a94f7951b54298ad4b3edb5d3822ceb2b466b591776",
    "pre_install": [
        "Rename-Item -Path \"$($dir)\\LunaTranslator\" -NewName \"dir\" -Force",
        "Get-ChildItem -Path \"$($dir)\\dir\" -Recurse | ForEach-Object { Move-Item -Path $_.FullName -Destination \"$($dir)\\$($_.Name)\" -Force }",
        "Remove-Item -Path \"$($dir)\\dir\" -Recurse -Force"
    ],
    "shortcuts": [
        [
            "LunaTranslator.exe",
            "LunaTranslator"
        ],
        [
            "LunaTranslator_admin.exe",
            "LunaTranslator (Admin)"
        ]
    ],
    "persist": [
        "userconfig",
        "translation_record",
        "cache"
    ],
    "notes": "LunaTranslator now only keeps the latest release assets, hold until CI update or use lunatranslator-latest.",
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/HIllya51/LunaTranslator/releases/download/v$version/LunaTranslator.zip"
    }
}
