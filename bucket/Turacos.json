{
    "version": "1.4.3",
    "description": "Turacos 是一款专业的多数据库安全评估工具，支持 PostgreSQL、MySQL、Redis、MSSQL 等多种数据库的后渗透操作。 为安全研究人员提供系统化、模块化的数据库安全测试能力，助力企业进行安全评估与漏洞验证。",
    "homepage": "https://github.com/alanni-tom/Turacos",
    "notes":[
        "漏洞利用",
        "后渗透,数据库,PostgreSQL、MySQL、Redis、MSSQL"
    ],
    "license": "UNKNOWN",
    "suggest": {
        "JDK": [
            "sec/oraclejdk"
        ]
    },
    "url": "https://github.com/alanni-tom/Turacos/releases/download/1.4.3/Turacos-1.4.3.zip",
    "hash": "",
    "pre_install": [
        "Set-Content \"$dir\\Turacos.bat\" '@pushd %~dp0",
        "@call oraclejdk",
        "@start javaw.exe --module-path \".\\javafx-sdk-17.0.12\\lib\" --add-modules javafx.controls,javafx.fxml -jar Turacos-1.0-SNAPSHOT.jar",
        "@popd' -Encoding Ascii"
    ],
    "bin": "Turacos.bat",
    "shortcuts": [
        [
            "Turacos.bat",
            "Turacos"
        ]
    ],
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/alanni-tom/Turacos/releases/download/$version/Turacos-$version.zip"
    }
}
