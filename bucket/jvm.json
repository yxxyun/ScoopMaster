{
    "version": "1.1.0",
    "description": "Java-Mocha is a Java version management tool developed based on the Foojay API.",
    "homepage": "https://github.com/morning-start/java-mocha",
    "license": "GNU3.0",
    "url": "https://github.com/morning-start/java-mocha/releases/download/v1.1.0/jvm-windows-1.1.0.zip",
    "hash": "29420e87fa9d1206b06edb1dad795b31a2cc6168a7c0a601dd7f17495f163aba",
    "bin": "jvm.exe",
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/morning-start/java-mocha/releases/download/v$version/jvm-windows-$version.zip"
    },
    "env_set": {
        "JVM_ROOT": "$persist_dir",
        "JAVA_HOME": "$persist_dir\\default"
    },
    "persist": [
        "jdk",
        "cache",
        "data",
        "default"
    ],
    "env_add_path": "default\\bin"
}
