{
    "version": "2025-08-26T05.53.42",
    "description": "在Burp的两端部署基于mitmproxy的上下游代理，以应对HTTP请求的加密传输问题，得益于mitmproxy为Python提供了丰富的API。",
    "homepage": "https://github.com/yinsel/BurpGuard",
    "license": "UNKNOWN",
    "suggest": {
        "python": "miniconda3"
    },
    "url": "https://github.com/yinsel/BurpGuard/archive/refs/heads/main.zip",
    "pre_install": [
        "Set-Content \"$dir\\BurpGuard.bat\" '@pushd %~dp0",
        "@call activate python3env",
        "@python \"BurpGuard.py\" %*",
        "@popd' -Encoding Ascii"
    ],
    "post_install": "& cmd /c \"call activate python3env && pip install -r $dir\\requirements.txt\"",
    "extract_dir": "BurpGuard-main",
    "bin": "BurpGuard.bat",
    "checkver": {
        "url": "https://api.github.com/repos/yinsel/BurpGuard/commits",
        "regex": "([\\d-]+T)(\\d+):(\\d+):(\\d+)",
        "replace": "$1$2.$3.$4"
    },
    "autoupdate": {
        "url": "https://github.com/yinsel/BurpGuard/archive/refs/heads/main.zip"
    }
}
