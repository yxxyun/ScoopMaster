        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe -Uninstaller 'Uninstall Cherry Studio.exe'",
        "A-Remove-TempData @(\"$env:LocalAppData\\cherrystudio-updater\")"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
