{
   "$schema": "https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
   "version": "1.0.24",
   "description": "A tool provides the Vagrant VMware provider plugin access to various VMware functionalities.",
   "homepage": "https://developer.hashicorp.com/vagrant/install/vmware",
   "license": "MPL-2.0",
   "architecture": {
      "64bit": {
         "url": "https://releases.hashicorp.com/vagrant-vmware-utility/1.0.24/vagrant-vmware-utility_1.0.24_windows_amd64.msi",
         "hash": "8ee7bea062d7552a484b7d9ade369744e5cd9ab1b55eed8b7bd35a1b006666d6"
      }
   },
   "installer": {
      "script": [
         "if (-not (is_admin)) { abort 'This package requires admin privileges to install' }",
         ". \"$dir\\vagrant-vmware-utility.exe\" certificate generate",
         ". \"$dir\\vagrant-vmware-utility.exe\" service install"
      ]
   },
   "uninstaller": {
      "script": [
         "if (-not (is_admin)) { abort 'This package requires admin privileges to uninstall' }",
         ". \"$dir\\vagrant-vmware-utility.exe\" service uninstall",
         "Remove-Item -Force -Recurse -Path \"$env:ProgramData\\hashicorp\\vagrant-vmware-desktop\""
      ]
   },
   "extract_dir": "VagrantVMwareUtility/bin",
   "bin": "vagrant-vmware-utility.exe",
   "checkver": {
      "url": "https://releases.hashicorp.com/vagrant-vmware-utility/",
      "regex": "vagrant-vmware-utility_([\\d.]+)</a>"
   },
   "autoupdate": {
      "architecture": {
         "64bit": {
            "url": "https://releases.hashicorp.com/vagrant-vmware-utility/$version/vagrant-vmware-utility_$version_windows_amd64.msi"
         }
      },
      "hash": {
         "url": "$baseurl/vagrant-vmware-utility_$version_SHA256SUMS"
      }
   }
}
