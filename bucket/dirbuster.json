    "extract_dir": "DirBuster-1.0-RC1",
    "bin": "DirBuster.bat",
    "shortcuts": [
        [
            "DirBuster.bat",
            "DirBuster"
        ]
    ],
    "pre_install": [
        "$content = Get-Content -Path $(Join-Path $dir 'DirBuster-1.0-RC1.bat')",
