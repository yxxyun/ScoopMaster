{
    "version": "1.3",
    "description": "具有交互式界面的自动 SSTI 检测工具",
    "notes": [
        "漏洞检测利用",
        "SSTI,模板注入"
    ],
    "homepage": "https://github.com/vladko312/SSTImap",
    "license": "GPL-3.0",
    "suggest": {
        "python": "miniconda3"
    },
    "url": "https://github.com/vladko312/SSTImap/archive/refs/tags/v1.3.zip",
    "hash": "1cc1f1d19068bc8f76c9a31b56584f185054dd133f6fa3e1c1206521f6b8414d",
    "pre_install": [
        "Set-Content \"$dir\\sstimap.bat\" '@pushd %~dp0",
        "@call activate python3env",
        "@python \"sstimap.py\" %*",
        "@popd' -Encoding Ascii"
    ],
    "post_install": [
        "Move-Item -Path \"$dir\\SSTImap-$version\\*\" -Destination \"$dir\\\"",
        "Remove-Item \"$dir\\SSTImap-$version\" -Force -Recurse",
        "& cmd /c \"call activate python3env && pip install -r $dir\\requirements.txt\""
    ],
    "bin": "sstimap.bat",
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/vladko312/SSTImap/archive/refs/tags/v$version.zip"
    }
}
