{
    "version": "0.5.2",
    "description": "An easy-to-learn/use static analysis framework for Java",
    "notes": "Java 静态分析框架",
    "homepage": "https://github.com/pascal-lab/Tai-e",
    "license": "Apache-2.0",
    "suggest": {
        "JRE": "sec/oraclejre8"
    },
    "url": "https://github.com/pascal-lab/Tai-e/releases/download/v0.5.2/tai-e-all-0.5.2.jar#/tai-e.jar",
    "pre_install": [
        "Set-Content \"$dir\\tai-e.bat\" '@pushd %~dp0",
        "@call oraclejre8",
        "@cmd /c java.exe -jar \"tai-e.jar\" %*",
        "@popd' -Encoding Ascii"
    ],
    "bin": "tai-e.bat",
    "shortcuts": [
        [
            "tai-e.bat",
            "Tai-e"
        ]
    ],
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/pascal-lab/Tai-e/releases/download/v$version/tai-e-all-$version.jar#/tai-e.jar"
    }
}
