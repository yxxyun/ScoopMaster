    },
    "shortcuts": [
        [
            "app-9.9.9999\\Discord.exe",
            "Discord"
        ]
    ],
    "checkver": {
        "url": "https://updates.discord.com/distributions/app/manifests/latest?channel=stable&platform=win&arch=x64",
        "jsonpath": "$.full.url",
