    "$bucket = $install.bucket",
    ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
    "A-Stop-Process",
    "A-Remove-Link",
    "A-Remove-TempData",
    "A-Complete-Uninstall"
  ],
