        }
    },
    "shortcuts": [
        [
            "PicList.exe",
            "PicList"
        ]
    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "A-Install-Exe"
    ],
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall",
        "$LinkPaths = @(\"$env:AppData\\piclist\")",
        "A-New-LinkDirectory $LinkPaths"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe -Uninstaller 'Uninstall PicList.exe'"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "github": "https://github.com/Kuingsmile/PicList"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Kuingsmile/PicList/releases/download/v$version/PicList-Setup-$version-x64.exe"
            },
