{
    "##": "来自https://github.com/hoilc/scoop-lemon/blob/master/bucket/piclist.json",
    "version": "3.2.2",
    "description": "An image upload and manage tool, base on PicGo.",
    "homepage": "https://piclist.cn/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://release.piclist.cn/latest/PicList-Setup-3.2.2-x64.exe#/dl.7z",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\"",
            "hash": "5048743289b3f718ad896a152ec03bba228eb86e2a08c09d98c011bcd36c940f"
        },
        "32bit": {
            "url": "https://release.piclist.cn/latest/PicList-Setup-3.2.2-ia32.exe#/dl.7z",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-32.7z\" \"$dir\"",
            "hash": "45380eda44c98d9e84d56b6caf5e907e47d4b000edc1fe4a305ff25168e0741f"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\", \"$dir\\resources\\app-update.yml\"  -Force -Recurse",
    "bin": "PicList.exe",
    "shortcuts": [
        [
            "PicList.exe",
            "PicList"
        ]
    ],
    "checkver": {
        "github": "https://github.com/Kuingsmile/PicList"
    },
    "installer": {
        "script": [
            ". \"$bucketsdir\\doge\\scripts\\utils.ps1\"",
            "Set-Junction -Source \"$persist_dir\\config\" -Target \"$env:APPDATA\\piclist\""
        ]
    },
    "uninstaller": {
        "script": [
            ". \"$bucketsdir\\doge\\scripts\\utils.ps1\"",
            "Remove-Junction -Path \"$env:APPDATA\\piclist\""
        ]
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://release.piclist.cn/latest/PicList-Setup-$version-x64.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://release.piclist.cn/latest/PicList-Setup-$version-ia32.exe#/dl.7z"
            }
        }
    }
}
