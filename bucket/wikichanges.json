{
    "version": "1.3.0",
    "description": "Wikidata changes fetcher.",
    "homepage": "https://github.com/the-qa-company/wiki-changes",
    "license": "https://github.com/the-qa-company/wiki-changes/blob/master/LICENSE.md",
    "suggest": {
        "JDK": "java/openjdk"
    },
    "url": "https://github.com/the-qa-company/wiki-changes/releases/download/v1.3.0/wikidata-changes.zip",
    "extract_dir": "wikidata-changes-1.3.0",
    "env_add_path": "bin",
    "checkver": {
        "url": "https://github.com/the-qa-company/wiki-changes/releases/latest",
        "regex": "Release ([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://github.com/the-qa-company/wiki-changes/releases/download/v$version/wikidata-changes.zip",
        "extract_dir": "wikidata-changes-$version",
        "env_add_path": "bin"
    }
}
