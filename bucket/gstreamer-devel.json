{
    "homepage": "https://gstreamer.freedesktop.org/",
    "version": "1.27.90",
    "description": "Open-source multimedia framework core library development files",
    "license": {
        "identifier": "LGPL-2.1",
        "url": "https://gitlab.freedesktop.org/gstreamer/gstreamer/-/raw/master/COPYING"
    },
    "architecture": {
        "64bit": {
            "url": "https://gstreamer.freedesktop.org/data/pkg/windows/1.27.90/msvc/gstreamer-1.0-devel-msvc-x86_64-1.27.90.msi",
            "hash": "b464684daa8eaafb6de0807bc61860bb568e488fb6d4e06011d3e2cfd6f6badf"
        },
        "32bit": {
            "url": "https://gstreamer.freedesktop.org/data/pkg/windows/1.27.90/msvc/gstreamer-1.0-devel-msvc-x86-1.27.90.msi",
            "hash": "de9256f9fe0db6c479f2ac7f7413b9d826e4741bc454d493b27ab3ad0a62fcd6"
        },
        "arm64": {
            "url": "https://gstreamer.freedesktop.org/data/pkg/windows/1.27.90/msvc/gstreamer-1.0-devel-msvc-arm64-1.27.90.msi",
            "hash": "168d3d8b466c132f2909683af23af081373d31754cb8bf5761f4483ca8e2cb26"
        }
    },
    "bin": [
        "bin/glib-compile-resources.exe",
        "bin/gresource.exe",
        "bin/orc-bugreport.exe",
        "bin/orcc.exe",
        "bin/xmllint.exe"
    ],
    "env_add_path": "bin",
    "env_set": {
        "PKG_CONFIG_PATH": "$dir/lib/pkgconfig;$env:PKG_CONFIG_PATH"
    },
    "checkver": {
        "url": "https://gstreamer.freedesktop.org/download/",
        "re": "(\\d+?\\.\\d+?\\.\\d+?) development installer"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://gstreamer.freedesktop.org/data/pkg/windows/$version/msvc/gstreamer-1.0-devel-msvc-x86_64-$version.msi"
            },
            "32bit": {
                "url": "https://gstreamer.freedesktop.org/data/pkg/windows/$version/msvc/gstreamer-1.0-devel-msvc-x86-$version.msi"
            },
            "arm64": {
                "url": "https://gstreamer.freedesktop.org/data/pkg/windows/$version/msvc/gstreamer-1.0-devel-msvc-arm64-$version.msi"
            }
        },
        "hash": {
            "url": "$url.sha256sum"
        }
    },
    "pre_install": [
        "if ($architecture -eq \"64bit\") { Move-Item -Path $dir/PFiles64/gstreamer/1.0/msvc_x86_64/* $dir  } elseif ($architecture -eq \"32bit\") { Move-Item -Path $dir/PFiles/gstreamer/1.0/msvc_x86/* $dir } elseif ($architecture -eq \"arm64\") { Move-Item -Path $dir/PFiles64/gstreamer/1.0/msvc_arm64/* $dir }",
        "if ($architecture -eq \"64bit\") { Remove-Item -Path $dir/PFiles64 -Force -Recurse } elseif ($architecture -eq \"32bit\") { Remove-Item -Path $dir/PFiles -Force -Recurse } elseif ($architecture -eq \"arm64\") { Remove-Item -Path $dir/PFiles64 -Force -Recurse }"
    ],
    "depends": "gstreamer"
}
