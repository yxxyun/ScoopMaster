{
    "version": "V1.1.2",
    "description": "Open source free packet capture tool，Support Windows、Mac、Android、IOS、Linux Full platform system",
    "homepage": "https://github.com/wanghongenpin/network_proxy_flutter",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/wanghongenpin/network_proxy_flutter/releases/download/V1.1.2/ProxyPin-Windows.zip",
            "hash": "373f5c9410cbb4b99a36f6bf2e5d0c06c538f12d6703c21e9eec0a54c36b2ab9"
        }
    },
    "extract_dir": "ProxyPin",
    "shortcuts": [
        [
            "ProxyPin.exe",
            "ProxyPin"
        ]
    ],
    "pre_install": [
        "Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name air) scripts/AirUtils.psm1)",
        "RedirectDirectory \"$env:APPDATA\\com.proxy.pin\" \"$persist_dir\\APPDATA\\com.proxy.pin\"",
        "RedirectDirectory \"$env:USERPROFILE\\.proxypin\" \"$persist_dir\\USERPROFILE\\.proxypin\"",
        "Remove-Module -Name AirUtils"
    ],
    "uninstaller": {
        "script": [
            "if ($cmd -eq 'uninstall') {",
            "    Import-Module $(Join-Path $(Find-BucketDirectory -Root -Name air) scripts/AirUtils.psm1)",
            "    RemoveJunction \"$env:APPDATA\\com.proxy.pin\"",
            "    RemoveJunction \"$env:USERPROFILE\\.proxypin\"",
            "    Remove-Module -Name AirUtils",
            "}"
        ]
    },
    "checkver": {
        "url": "https://github.com/wanghongenpin/network_proxy_flutter/releases/latest",
        "regex": "/network_proxy_flutter/tree/v?([^/]+?)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/wanghongenpin/network_proxy_flutter/releases/download/$version/ProxyPin-Windows.zip"
            }
        }
    }
}
