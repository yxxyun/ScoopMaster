    "pre_install": [
        "Set-Content \"$dir\\ParamWizard.bat\" '@pushd %~dp0",
        "@python3 \"ParamWizard.py\" %*",
        "@popd' -Encoding Ascii"
    ],
    "post_install": [
        "Move-Item -Path \"$dir\\ParamWizard-main\\*\" -Destination \"$dir\\\"",
        "Remove-Item \"$dir\\ParamWizard-main\" -Force -Recurse",
        "pip3 install -r \"$dir\\requirements.txt\""
    ],
    "bin": "ParamWizard.bat",
