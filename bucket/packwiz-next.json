{
    "version": "0.0.6+66120cd.20616145220",
    "description": "A command line tool for editing and distributing Minecraft modpacks, using a git-friendly TOML format. Supports CurseForge and Modrinth mods with automated updates!",
    "homepage": "https://github.com/QianFuv/packwiz-next",
    "license": {
        "identifier": "MIT",
        "url": "https://github.com/QianFuv/packwiz-next/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://nightly.link/QianFuv/packwiz-next/actions/runs/20616145220/packwiz-next%20Windows%2064-bit.zip",
            "hash": "013d40ba30797c00a43e751f0bbebfa0e961c4cc662bc9f5629be7399cb81b3a"
        }
    },
    "checkver": {
        "script": [
            "$actions = Invoke-RestMethod -Uri 'https://api.github.com/repos/QianFuv/packwiz-next/actions/workflows/go.yml/runs'",
            "$latest_develop = $actions.workflow_runs | Where-Object { $_.head_branch -eq 'main' } | Where-Object { $_.conclusion -eq 'success' } | Select-Object -first 1",
            "$version = '0.0.' + $latest_develop.run_number + '+' + $latest_develop.head_sha.substring(0,7) + '.' + $latest_develop.id",
            "Write-Output $version $latest_develop.id"
        ],
        "regex": "(?<version>[^\\s]+)\\s(?<id>\\d+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://nightly.link/QianFuv/packwiz-next/actions/runs/$matchId/packwiz-next%20Windows%2064-bit.zip"
            }
        }
    },
    "bin": "packwiz-next.exe"
}
