{
  "version": "7.0.0",
  "##": "https://github.com/dotnet/core/blob/main/release-notes/7.0/7.0.0/7.0.0.md",
  "homepage": "https://dotnet.microsoft.com/download/dotnet-core",
  "architecture": {
    "32bit": {
      "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/7.0.0/windowsdesktop-runtime-7.0.0-win-x86.exe"
    },
    "64bit": {
      "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/7.0.0/windowsdesktop-runtime-7.0.0-win-x64.exe"
    }
  },  
  "bin": "dotnet.exe",
  "installer": {
    "script": [
      "Expand-DarkArchive \"$dir\\$fname\" \"$dir\\extracted\" -Removal",
      "Get-ChildItem \"$dir\\extracted\\AttachedContainer\\*.msi\" | ForEach-Object { Expand-MsiArchive $_ \"$dir\" }",
      "Move-Item \"$dir\\dotnet\\*\" \"$dir\" -Force",
      "@('dotnet', 'extracted') | ForEach-Object { Remove-Item \"$dir\\$_\" -Recurse }"
    ]
  },
  "depends": "dark",
  "description": "Microsoft .NET 7 Desktop Runtime.",
  "env_set": {
    "DOTNET_ROOT": "$dir"
  },
  "license": {
    "identifier": "Freeware",
    "url": "https://www.microsoft.com/en-us/legal/intellectualproperty/copyright/default.aspx"
  },
  "notes": "This package only contains the .NET 7.0.0 desktop runtime, install 'dotnet-sdk' instead if you need .NET Core SDK"
}
