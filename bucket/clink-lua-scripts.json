{
   "$schema": "https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
   "version": "1.0.0",
   "description": "Install Clink scripts. This installs scripts under `~/.config/clink`.",
   "depends": "main/clink",
   "url": "https://raw.githubusercontent.com/silver886/scoop/master/bucket/clink-lua-scripts.json",
   "hash": "2b730083b890438767fc9565f889d453e535a6428a1595dd629a6c5e7bf84e95",
   "installer": {
      "script": "clink installscripts \"$env:UserProfile\\.config\\clink\""
   },
   "uninstaller": {
      "script": [
         "if (Get-Command clink -ErrorAction SilentlyContinue) {",
         "   clink uninstallscripts \"$env:UserProfile\\.config\\clink\"",
         "}"
      ]
   },
   "checkver": {
      "url": "https://raw.githubusercontent.com/silver886/scoop/master/bucket/clink-lua-scripts.json",
      "jsonpath": "$.version"
   },
   "autoupdate": {
      "url": "https://raw.githubusercontent.com/silver886/scoop/master/bucket/clink-lua-scripts.json"
   }
}
