{
    "##": [
        "Why here? No existing request for it.",
        "Homebrew has it though: <https://formulae.brew.sh/cask/edfbrowser#default>."
    ],
    "version": "2.14",
    "description": "A free, open-source, multiplatform, universal viewer, annotator and toolbox intended for, but not limited to, time-series storage files like EEG, EMG, ECG, BioImpedance, etc.",
    "homepage": "https://www.teuniz.net/edfbrowser/",
    "license": "GPL-3.0-only",
    "architecture": {
        "64bit": {
            "url": "https://www.teuniz.net/edfbrowser/setup_edfbrowser_214_x86_64-w64.zip",
            "hash": "6b23a724be1e24031764515330f9391ad6c7dedfa1fb86c328ba89cfa8c6f4a2"
        }
    },
    "pre_install": [
        "$ExeFile = [string]::Format(",
        "    '{0}\\{1}',",
        "    $dir,",
        "    $Manifest.architecture.$architecture.url.Split('/')[-1].Replace('.zip', '.exe')",
        ")",
        "Expand-7zipArchive -Path \"$ExeFile\" -DestinationPath \"$dir\"",
        "Remove-Item -Path \"$ExeFile\", \"$dir\\uninst.exe\", \"$dir\\`$PLUGINSDIR\" -Recurse -Force"
    ],
    "shortcuts": [
        [
            "edfbrowser.exe",
            "EDFBrowser"
        ]
    ],
    "checkver": {
        "url": "https://www.teuniz.net/edfbrowser/version.txt",
        "regex": "version (?<version>[\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://www.teuniz.net/edfbrowser/setup_edfbrowser_$cleanVersion_x86_64-w64.zip",
                "hash": {
                    "url": "$url.sha256"
                }
            }
        }
    }
}
