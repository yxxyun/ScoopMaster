{
    "version": "1",
    "description": "Add the Strawberry Perl compiled binaries to the PATH.",
    "url": "https://raw.githubusercontent.com/vlmantova/Scoop-LaTeXML/refs/heads/master/deprecated/.gitkeep",
    "hash": "44a5a53e8a062a01cd7134565b5300a9242e369545639329dc0204ed0e5b05c0",
    "depends": "perl",
    "installer": {
        "script": "Add-Path (((Test-Path (appdir 'perl' $Global)) ? (appdir 'perl' $Global) : (appdir 'perl' $true)) + \"\\current\\c\\bin\")"
    },
    "pre_uninstall": "Remove-Path (((Test-Path (appdir 'perl' $Global)) ? (appdir 'perl' $Global) : (appdir 'perl' $true)) + \"\\current\\c\\bin\")"
}
