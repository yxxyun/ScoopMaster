{
    "version": "11.2.0",
    "description": "一个开源的跨平台 API 客户端，适用于 GraphQL、REST、WebSockets、SSE 和 gRPC。具有 Cloud、Local 和 Git 存储。The open-source, cross-platform API client for GraphQL, REST, WebSockets, SSE and gRPC. With Cloud, Local and Git storage.",
    "homepage": "https://insomnia.rest",
    "license": {
        "identifier": "Apache License 2.0",
        "url": "https://github.com/Kong/insomnia/blob/develop/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/Kong/insomnia/releases/download/core%4011.2.0/insomnia-11.2.0-full.nupkg",
            "hash": "sha1:2a3749b6fd0dc99510cd393b600b2700758e9fe4"
        }
    },
    "extract_dir": "lib\\net45",
    "shortcuts": [
        [
            "Insomnia.exe",
            "Insomnia"
        ]
    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall"
    ],
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall",
        "A-New-LinkDirectory @(\"$env:AppData\\Insomnia\")"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Remove-TempData"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "url": "https://api.github.com/repositories/56899284/releases",
        "regex": "\"core@([\\d.]+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/Kong/insomnia/releases/download/core%40$version/insomnia-$version-full.nupkg",
                "hash": {
                    "url": "$baseurl/RELEASES"
                }
            }
        }
    }
}
