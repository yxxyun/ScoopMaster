{
    "version": "1.8.0",
    "description": "Golang Version Manager",
    "homepage": "https://github.com/voidint/g",
    "license": "MIT",
    "notes": [
        "go 设置 官方代理",
        "go env -w  GOPROXY=https://goproxy.io,direct",
        "阿里云: https://mirrors.aliyun.com/goproxy/,direct",
        "七牛CDN: https://goproxy.cn,direct"
    ],
    "architecture": {
        "64bit": {
            "url": "https://github.com/voidint/g/releases/download/v1.8.0/g1.8.0.windows-amd64.zip",
            "hash": "93ad318012c8dc601b93ad44ce82b10fbdb23e169d831bb041061c0f6783c7e9"
        }
    },
    "bin": [
        [
            "g.exe",
            "gvm"
        ]
    ],
    "env_set": {
        "G_MIRROR": "https://mirrors.aliyun.com/golang",
        "G_EXPERIMENTAL": "true",
        "G_HOME": "$dir\\.g",
        "GOROOT": "$persist_dir\\.g\\go",
        "GOCACHE": "$dir\\.go\\GOCACHE",
        "GOENV": "$dir\\.go\\GOENV",
        "GOPATH": "$dir\\.go\\GOPATH"
    },
    "env_add_path": [
        ".g\\go\\bin",
        ".go\\GOPATH\\bin"
    ],
    "persist": [
        ".g",
        ".go"
    ],
    "checkver": "github",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/voidint/g/releases/download/v$version/g$version.windows-amd64.zip"
            },
            "32bit": {
                "url": "https://github.com/voidint/g/releases/download/v$version/g$version.windows-386.zip"
            }
        }
    }
}
