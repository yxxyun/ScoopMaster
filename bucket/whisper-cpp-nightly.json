            "whisper-cpp-server"
        ],
        [
            "whisper-stream.exe",
            "whisper-cpp-stream"
        ],
        [
            "whisper-talk-llama.exe",
            "whisper-cpp-talk-llama"
        ]
    ],
    "checkver": {
        "url": "https://github.com/ggerganov/whisper.cpp/actions/workflows/build.yml?query=branch%3Amaster+is%3Asuccess",
        "regex": "(?sm)/actions/runs/(?<run>\\d+)\".*?#(\\d+):"
    },
    "autoupdate": {
        "architecture": {
