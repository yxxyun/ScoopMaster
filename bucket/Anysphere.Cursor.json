{
    "version": "1.2.0",
    "description": "一个 AI 代码编辑器。The AI Code Editor.",
    "homepage": "https://www.cursor.com/",
    "license": "Freemium",
    "architecture": {
        "64bit": {
            "url": "https://downloads.cursor.com/production/eb5fa4768da0747b79dc34f0b79ab20dbf58202c/win32/x64/user-setup/CursorUserSetup-x64-1.2.0.exe",
            "hash": "d5f588c3580e8e40d84afa6bc75514307dee161ad43355ac59cddd07090010b9"
        },
        "arm64": {
            "url": "https://downloads.cursor.com/production/eb5fa4768da0747b79dc34f0b79ab20dbf58202c/win32/arm64/user-setup/CursorUserSetup-arm64-1.2.0.exe",
            "hash": "cd89c6c40974eb7922c4175f4eaaf760b9ba97e7b360a52d9f0b6d783b34add2"
        }
    },
    "innosetup": true,
    "extract_dir": "{code_GetDestDir}",
    "bin": [
        [
            "resources\\app\\bin\\cursor.cmd",
            "cursor"
        ]
    ],
    "shortcuts": [
        [
            "Cursor.exe",
            "Cursor"
        ]
    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall"
    ],
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall",
        "A-New-LinkDirectory @(\"$env:AppData\\Cursor\",\"$env:UserProfile\\.cursor\")"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Stop-Process",
        "A-Remove-Link"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'Anysphere.Cursor' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_user.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_user.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_user.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        }
    }
}
