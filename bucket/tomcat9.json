{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.110",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.110",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.110/bin/apache-tomcat-9.0.110-windows-x64.zip",
            "hash": "sha512:c887ed41fa222bd74626bec9fc199956d2d697e17a0b4f65a2bf13c8c57b842a0bcc622f66b60e2f6041be58454d6471fcb431b282ed0e58ff086e9c3b712083"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.110/bin/apache-tomcat-9.0.110-windows-x86.zip",
            "hash": "sha512:3418b507bdf02dfbecb09d1c5c29583f00030881763a54512f798fd24efe1da24417826812f8f6e9463623630a8ec6db2f469fab3e2a239beda6a6a3b94391b3"
        }
    }
}
