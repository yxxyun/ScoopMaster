{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.100",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.100",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.100/bin/apache-tomcat-9.0.100-windows-x64.zip",
            "hash": "sha512:056963e08843ade9b34031028a18b8c52c76399b3024b24cb30760cba6c8444aefc9859cd4e59717bb10413f6f5db6b0c91682faf59b7654ffb0834f6f5da9e3"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.100/bin/apache-tomcat-9.0.100-windows-x86.zip",
            "hash": "sha512:a638992659ff598be6497de6d3cd7a46428db57e0be46647e702f7f597d2ed965309d6fd01684cf64ff7fb74b41c6ad55540ce77f93ad3f0ad3c3ddd859a6412"
        }
    }
}
