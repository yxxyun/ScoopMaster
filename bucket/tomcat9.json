{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.113",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.113",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.113/bin/apache-tomcat-9.0.113-windows-x64.zip",
            "hash": "sha512:a80c4176f0e9e2c3f4557fbd2d68a2dd91695f6aff7ebb2f4217b19e9a0bfb9af1cb1e59663a4b74822e7cce6a5a50195053ca2ea0a6e0c5e3d76fc5aa5339ca"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.113/bin/apache-tomcat-9.0.113-windows-x86.zip",
            "hash": "sha512:ab6ab5ba99bf8ba08a1a250bb55d3ad175ea55866a1befd37382be8be8e79172e2b9d7439ac5903601675d988c09bb052a8bd89954591a31363b41d21421f178"
        }
    }
}
