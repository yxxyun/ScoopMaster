        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.112/bin/apache-tomcat-9.0.112-windows-x86.zip",
            "hash": "sha512:01cc5c3f1761dbcb320d07755c66440593c8ff0c27a8e0834a8e3761191f70ed0f10267bbc56cf386985b4f3abe131eb92083c8f19e3463f056b1307e38ead4a"
        }
    }
}
