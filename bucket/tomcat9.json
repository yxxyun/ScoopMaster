{
    "homepage": "https://tomcat.apache.org",
    "version": "9.0.99",
    "checkver": {
        "url": "https://www.apache.org/dist/tomcat/tomcat-9/?C=M;O=D",
        "re": "v(?<version>[\\d.]+)"
    },
    "autoupdate": {
        "extract_dir": "apache-tomcat-$version",
        "architecture": {
            "32bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x86.zip"
            },
            "64bit": {
                "url": "https://www.apache.org/dist/tomcat/tomcat-9/v$version/bin/apache-tomcat-$version-windows-x64.zip"
            }
        },
        "hash": {
            "url": "$url.sha512"
        }
    },
    "extract_dir": "apache-tomcat-9.0.99",
    "bin": "bin\\catalina.bat",
    "env_set": {
        "CATALINA_HOME": "$dir",
        "CATALINA_BASE": "$dir"
    },
    "suggest": {
        "JRE": "java/openjdk"
    },
    "architecture": {
        "64bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.99/bin/apache-tomcat-9.0.99-windows-x64.zip",
            "hash": "sha512:b39199044d3ad6b41abe651f630da00e7dc60ce9e530e18a307d64877961a4c1e269c84f20aba12441095ab5b6a846b0190530f4f213c29262d003cdfeaf19f6"
        },
        "32bit": {
            "url": "https://www.apache.org/dist/tomcat/tomcat-9/v9.0.99/bin/apache-tomcat-9.0.99-windows-x86.zip",
            "hash": "sha512:437382287e60096d5607f920a221fb9d887997e27be5d5b7eccb9ce272fc6543d63ded3ccfaff32a687e0ade15160dbb1566902b06b9c3ff23d57461e43ef741"
        }
    }
}
