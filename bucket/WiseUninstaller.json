{
    "version": "3.2.3.266",
    "description": "No need to install it. You can use this small and clean tool to remove software from your PC.",
    "homepage": "https://www.wisecleaner.com/wise-program-uninstaller.html",
    "license": "Freeware",
    "url": "http://downloads.wisecleaner.com/soft/WPU_3.2.3.266.zip",
    "hash": "892cf7a19dd21c3bef7112320f8c2095d8e03d419cf993be89eec5e3e2a0f818",
    "installer": {
        "script": [
            "if (-not (Test-Path \"$persist_dir\\config.ini\")) { ",
            "  New-Item -Force -Path \"$dir\\config.ini\" | Out-Null",
            "  echo \"[General]\" >> \"$dir\\config.ini\"",
            "  echo \"AutoUpdate=0\" >> \"$dir\\config.ini\"",
            "  echo \"CheckVer=0\" >> \"$dir\\config.ini\"",
            "  echo \"SearchLeft=1\" >> \"$dir\\config.ini\"",
            "  echo \"submit=0\" >> \"$dir\\config.ini\"",
            "  echo \"CreateRestorePoint=0\" >> \"$dir\\config.ini\"",
            "  echo \"MsgIDs=,687\" >> \"$dir\\config.ini\"",
            "  echo \"UEI=0\" >> \"$dir\\config.ini\"",
            "  echo \"getad=0\" >> \"$dir\\config.ini\"",
            "  echo \"[GUI]\" >> \"$dir\\config.ini\"",
            "  echo \"SaveLeftOver=0\" >> \"$dir\\config.ini\"",
            "  echo \"Delecting=0\" >> \"$dir\\config.ini\"",
            "  echo \"DelLeftOver=0\" >> \"$dir\\config.ini\"",
            " }"
        ]
    },
    "extract_dir": "WPUSetup_3.2.3.266",
    "shortcuts": [
        [
            "WiseProgramUninstaller.exe",
            "WiseUninstaller"
        ]
    ],
    "persist": [
        "config.ini",
        "backup"
    ],
    "checkver": {
        "url": "https://www.wisecleaner.com/download.html",
        "regex": "WPU.+?([\\d.]+)?\\.zip"
    },
    "autoupdate": {
        "url": "http://downloads.wisecleaner.com/soft/WPU_$version.zip",
        "extract_dir": "WPUSetup_$version"
    }
}
