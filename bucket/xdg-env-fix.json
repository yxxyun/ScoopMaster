{
   "$schema": "https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
   "version": "1.0.0",
   "description": "Fix XDG env. This set `XDG_DATA_HOME`, `XDG_CONFIG_HOME`, `XDG_STATE_HOME`, and `XDG_CACHE_HOME`.",
   "url": "https://raw.githubusercontent.com/silver886/scoop/master/LICENSE",
   "hash": "7e12e5df4bae12cb21581ba157ced20e1986a0508dd10d0e8a4ab9a4cf94e85c",
   "env_set": {
      "XDG_DATA_HOME": "$env:UserProfile\\.local\\share",
      "XDG_CONFIG_HOME": "$env:UserProfile\\.config",
      "XDG_STATE_HOME": "$env:UserProfile\\.local\\state",
      "XDG_CACHE_HOME": "$env:UserProfile\\.cache"
   }
}
