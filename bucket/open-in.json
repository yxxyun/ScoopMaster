{
    "version": "1.0.7",
    "description": "Native client component to allow 'Open in ...' browser extensions to launch local applications via a messaging host.",
    "homepage": "https://github.com/andy-portmen/native-client",
    "license": "MIT",
    "url": "https://github.com/andy-portmen/native-client/releases/download/v1.0.7/windows.zip",
    "hash": "b8b519b51fd71a6bf8675c665a7176509ad64bb283db4882a5cb9bbc9c8094fe",
    "notes": [
        "After install, run the native host registration script:",
        "  & \"$(scoop prefix open-in)\\install.bat\"",
        "To uninstall the native host prior to 'scoop uninstall open-in':",
        "  & \"$(scoop prefix open-in)\\uninstall.bat\""
    ],
    "post_install": "Write-Host \"Run '& \\\"$((scoop prefix open-in))\\install.bat\\\"' to finish setup.\"",
    "pre_uninstall": [
        "$uninstallPath = \"$(scoop prefix open-in)\\uninstall.bat\"",
        "if (Test-Path $uninstallPath) { & $uninstallPath }"
    ],
    "checkver": {
        "github": "https://github.com/andy-portmen/native-client",
        "regex": "tag/v?([\\d.]+)"
    },
    "autoupdate": {
        "url": "https://github.com/andy-portmen/native-client/releases/download/v$version/windows.zip"
    }
}
