{
    "version": "9.80",
    "url": "https://files.chemtable.com/ro/portable/reg-organizer-portable-setup.exe",
    "innosetup": "true",
    "checkver": {
        "url": "https://www.chemtable.com/organizer.htm",
        "regex": "([\\d.]+)"
    },
    "autoupdate": {
        "version": "$version"
    },
    "installer": {
    "script": [
        "Copy-Item \"$dir\\{app}\\*\" \"$dir\" -Force -Recurse | Out-Null",
        "Remove-Item \"$dir\\{app}\" -Force -Recurse | Out-Null",
        "Remove-Item \"$dir\\{tmp}\" -Force -Recurse | Out-Null",
        "Remove-Item \"$dir\\install_script.iss\" -Force -Recurse | Out-Null"
        ]
    }
}
