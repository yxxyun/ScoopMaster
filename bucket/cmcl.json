{
    "version": "2.2.2",
    "description": "Command-line tool to interact with appbase.io",
    "homepage": "https://github.com/MrShieh-X/cmcl",
    "license": "Apache-2.0",
    "architecture": {
        "64bit": {
            "url": "https://github.com/MrShieh-X/cmcl-kotlin/releases/download/2.2.2/cmcl.exe"
        }
    },
    "pre_install": "Rename-Item \"$dir\\cmcl-$version.exe\" 'cmcl.exe' -ErrorAction SilentlyContinue",
    "bin": "cmcl.exe",
    "checkver": {
        "github": "https://github.com/MrShieh-X/cmcl",
        "regex": "tag/([\\w.-]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/MrShieh-X/cmcl-kotlin/releases/download/$version/cmcl.exe"
            }
        }
    }
}
