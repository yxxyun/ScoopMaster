  "hash": "4a36b2e6f65dbf35045c28ed19a50d736fe24139967931bcc293108ca3a0d3ac",
  "shortcuts": [
    [
      "SwitchHosts.exe",
      "SwitchHosts"
    ]
  ],
  "pre_install": [
    ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\""
  ],
  "post_install": [
    ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
    "$LinkPaths = @(\"$env:UserProfile\\.SwitchHosts\")",
    "$LinkTargets = @(\"$persist_dir\\.SwitchHosts\")",
    "A-New-LinkDirectory $LinkPaths $LinkTargets",
    "A-Complete-Install"
  ],
  "pre_uninstall": [
    "$bucket = $install.bucket",
    ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
    "A-Stop-Process",
    "A-Remove-Link",
    "A-Remove-TempData @(\"$env:AppData\\SwitchHosts\")",
    "A-Complete-Uninstall"
  ],
  "checkver": {
    "github": "https://github.com/oldj/SwitchHosts"
  },
  "autoupdate": {
    "url": "https://github.com/oldj/SwitchHosts/releases/download/v$version/SwitchHosts-$version-win.zip"
  }
}
