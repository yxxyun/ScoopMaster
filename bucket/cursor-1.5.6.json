{
    "version": "1.5.6",
    "description": "The AI Code Editor",
    "homepage": "https://www.cursor.com",
    "license": {
        "identifier": "Proprietary",
        "url": "https://www.cursor.com/terms-of-service"
    },
    "innosetup": true,
    "architecture": {
        "64bit": {
            "url": "https://downloads.cursor.com/production/6eed2e036b92b60f0cef5b52a2e1b99f3358bca3/win32/x64/user-setup/CursorUserSetup-x64-1.5.6.exe",
            "hash": "ceeb362ccd02b8584ff2754c81548733572328ba22bec16abf104e6326d0e718"
        },
        "arm64": {
            "url": "https://downloads.cursor.com/production/6eed2e036b92b60f0cef5b52a2e1b99f3358bca3/win32/arm64/user-setup/CursorUserSetup-arm64-1.5.6.exe",
            "hash": "58f85d36288eb7c485c444bcc98e5890e9ac8bfac1a1c45d712d581196e0657b"
        }
    },
    "extract_dir": "{code_GetDestDir}",
    "post_install": [
        "Remove-Item \"$dir\\`$*\", \"$dir\\Uninst*\" -Recurse",
        "if (!(Test-Path \"$dir\\data\\extensions\") -and (Test-Path \"$env:USERPROFILE\\.cursor\\extensions\")) {",
        "    info '[Portable Mode] Copying extensions...'",
        "    Copy-Item \"$env:USERPROFILE\\.cursor\\extensions\" \"$dir\\data\" -Recurse",
        "}",
        "if (!(Test-Path \"$dir\\data\\user-data\") -and (Test-Path \"$env:AppData\\Cursor\")) {",
        "    info '[Portable Mode] Copying user data...'",
        "    Copy-Item \"$env:AppData\\Cursor\" \"$dir\\data\\user-data\" -Recurse",
        "}",
        "$extensions_file = \"$dir\\data\\extensions\\extensions.json\"",
        "if ((Test-Path \"$extensions_file\")) {",
        "    info 'Adjusting path in extensions file...'",
        "    (Get-Content \"$extensions_file\") -replace '(?<=cursor(/|\\\\\\\\)).*?(?=(/|\\\\\\\\)data(/|\\\\\\\\)extensions)', $version | Set-Content \"$extensions_file\"",
        "}"
    ],
    "bin": [
        [
            "resources\\app\\bin\\cursor.cmd",
            "cursor_1.5.6",
            "--user-data-dir=\"$dir\\data\\user-data\" --extensions-dir=\"$dir\\data\\extensions\""
        ]
    ],
    "shortcuts": [
        [
            "Cursor.exe",
            "Cursor 1.5.6",
            "--user-data-dir=\"$dir\\data\\user-data\" --extensions-dir=\"$dir\\data\\extensions\""
        ]
    ],
    "persist": "data"
}
