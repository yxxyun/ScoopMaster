{
   "$schema": "https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
   "version": "1.0.0",
   "description": "Fix UniExtract2 bin. This .",
   "depends": "extras/uniextract2",
   "suggest": {
      "Inno Setup Unpacker": "main/innounp",
      "WiX Decompiler": "main/dark",
      "Windows Installer extractor": "main/lessmsi",
      "7-Zip": "main/7zip",
      "UnRAR": "main/unrar"
   },
   "url": "https://raw.githubusercontent.com/silver886/scoop/master/LICENSE",
   "hash": "7e12e5df4bae12cb21581ba157ced20e1986a0508dd10d0e8a4ab9a4cf94e85c",
   "installer": {
      "script": [
         "$UniExtract2BinDir = \"$(appdir uniextract2)\\current\\bin\"",
         "Get-ChildItem -Path \"$(shimdir)\\innounp*\" | Foreach-Object {",
         "   Move-Item -Path \"$UniExtract2BinDir\\$($_.Name)\" -Destination \"$UniExtract2BinDir\\$($_.Name).bak\" > $null",
         "   Copy-Item -Path \"$($_.FullName)\" -Destination \"$UniExtract2BinDir\\$($_.Name)\" > $null",
         "}",
         "Get-ChildItem -Path \"$(shimdir)\\dark*\" | Foreach-Object {",
         "   Move-Item -Path \"$UniExtract2BinDir\\dark\\$($_.Name)\" -Destination \"$UniExtract2BinDir\\dark\\$($_.Name).bak\" > $null",
         "   Copy-Item -Path \"$($_.FullName)\" -Destination \"$UniExtract2BinDir\\dark\\$($_.Name)\" > $null",
         "}",
         "Get-ChildItem -Path \"$(shimdir)\\lessmsi*\" | Foreach-Object {",
         "   Move-Item -Path \"$UniExtract2BinDir\\lessmsi\\$($_.Name)\" -Destination \"$UniExtract2BinDir\\lessmsi\\$($_.Name).bak\" > $null",
         "   Copy-Item -Path \"$($_.FullName)\" -Destination \"$UniExtract2BinDir\\lessmsi\\$($_.Name)\" > $null",
         "}",
         "Get-ChildItem -Path \"$(shimdir)\\7z*\" | Foreach-Object {",
         "   Move-Item -Path \"$UniExtract2BinDir\\x64\\$($_.Name)\" -Destination \"$UniExtract2BinDir\\x64\\$($_.Name).bak\" > $null",
         "   Copy-Item -Path \"$($_.FullName)\" -Destination \"$UniExtract2BinDir\\x64\\$($_.Name)\" > $null",
         "}",
         "Get-ChildItem -Path \"$(shimdir)\\unrar*\" | Foreach-Object {",
         "   Move-Item -Path \"$UniExtract2BinDir\\x64\\$($_.Name)\" -Destination \"$UniExtract2BinDir\\x64\\$($_.Name).bak\" > $null",
         "   Copy-Item -Path \"$($_.FullName)\" -Destination \"$UniExtract2BinDir\\x64\\$($_.Name)\" > $null",
         "}"
      ]
   },
   "uninstaller": {
      "script": [
         "$UniExtract2BinDir = \"$(appdir uniextract2)\\current\\bin\"",
         "Get-ChildItem -Path \"$(shimdir)\\innounp*\" | Foreach-Object {",
         "   Remove-Item -Path \"$UniExtract2BinDir\\$($_.Name)\" > $null",
         "   Move-Item -Path \"$UniExtract2BinDir\\$($_.Name).bak\" -Destination \"$UniExtract2BinDir\\$($_.Name)\" > $null",
         "}",
         "Get-ChildItem -Path \"$(shimdir)\\dark*\" | Foreach-Object {",
         "   Remove-Item -Path \"$UniExtract2BinDir\\dark\\$($_.Name)\" > $null",
         "   Move-Item -Path \"$UniExtract2BinDir\\dark\\$($_.Name).bak\" -Destination \"$UniExtract2BinDir\\dark\\$($_.Name)\" > $null",
         "}",
         "Get-ChildItem -Path \"$(shimdir)\\lessmsi*\" | Foreach-Object {",
         "   Remove-Item -Path \"$UniExtract2BinDir\\lessmsi\\$($_.Name)\" > $null",
         "   Move-Item -Path \"$UniExtract2BinDir\\lessmsi\\$($_.Name).bak\" -Destination \"$UniExtract2BinDir\\lessmsi\\$($_.Name)\" > $null",
         "}",
         "Get-ChildItem -Path \"$(shimdir)\\7z*\" | Foreach-Object {",
         "   Remove-Item -Path \"$UniExtract2BinDir\\x64\\$($_.Name)\" > $null",
         "   Move-Item -Path \"$UniExtract2BinDir\\x64\\$($_.Name).bak\" -Destination \"$UniExtract2BinDir\\x64\\$($_.Name)\" > $null",
         "}",
         "Get-ChildItem -Path \"$(shimdir)\\unrar*\" | Foreach-Object {",
         "   Remove-Item -Path \"$UniExtract2BinDir\\x64\\$($_.Name)\" > $null",
         "   Move-Item -Path \"$UniExtract2BinDir\\x64\\$($_.Name).bak\" -Destination \"$UniExtract2BinDir\\x64\\$($_.Name)\" > $null",
         "}"
      ]
   }
}
