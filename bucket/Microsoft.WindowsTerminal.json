{
    "version": "1.22.11751.0",
    "description": "新的 Windows 终端，为 Windows 提供标签命令行体验。The new Windows Terminal, a tabbed command line experience for Windows.",
    "homepage": "https://github.com/microsoft/terminal",
    "license": {
        "identifier": "MIT",
        "url": "https://github.com/microsoft/terminal/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/microsoft/terminal/releases/download/v1.22.11751.0/Microsoft.WindowsTerminal_1.22.11751.0_8wekyb3d8bbwe.msixbundle",
            "hash": "dcee05c52d5732d0459614eaa449aa054a3ea9d9d148863e7da3f0a02ccc8022"
        },
        "32bit": {
            "url": "https://github.com/microsoft/terminal/releases/download/v1.22.11751.0/Microsoft.WindowsTerminal_1.22.11751.0_8wekyb3d8bbwe.msixbundle",
            "hash": "dcee05c52d5732d0459614eaa449aa054a3ea9d9d148863e7da3f0a02ccc8022"
        },
        "arm64": {
            "url": "https://github.com/microsoft/terminal/releases/download/v1.22.11751.0/Microsoft.WindowsTerminal_1.22.11751.0_8wekyb3d8bbwe.msixbundle",
            "hash": "dcee05c52d5732d0459614eaa449aa054a3ea9d9d148863e7da3f0a02ccc8022"
        }
    },
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "$parentId = (Get-Process | Where-Object { $_.Id -eq $PID }).Parent.Id",
        "$processName = (Get-Process | Where-Object { $_.Id -eq $parentId }).ProcessName",
        "if ($processName -eq 'WindowsTerminal') {",
        "    if ($PSUICulture -like 'zh*') {",
        "       Write-Host \"不能在 WindowsTerminal 中安装、更新、卸载 WindowsTerminal\" -ForegroundColor Red",
        "    }",
        "    else {",
        "       Write-Host \"You cannot $cmd WindowsTerminal in WindowsTerminal.\" -ForegroundColor Red",
        "    }",
        "   exit 1",
        "}"
    ],
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall",
        "$PackageFamilyName = 'Microsoft.WindowsTerminal_8wekyb3d8bbwe'",
        "A-Add-MsixPackage",
        "A-New-LinkDirectory @(\"$env:LocalAppData\\Packages\\$PackageFamilyName\\LocalState\")"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "$parentId = (Get-Process | Where-Object { $_.Id -eq $PID }).Parent.Id",
        "$processName = (Get-Process | Where-Object { $_.Id -eq $parentId }).ProcessName",
        "if ($processName -eq 'WindowsTerminal') {",
        "    if ($PSUICulture -like 'zh*') {",
        "       Write-Host \"不能在 WindowsTerminal 中安装、更新、卸载 WindowsTerminal\" -ForegroundColor Red",
        "    }",
        "    else {",
        "       Write-Host \"You cannot $cmd WindowsTerminal in WindowsTerminal.\" -ForegroundColor Red",
        "    }",
        "   exit 1",
        "}",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Remove-MsixPackage"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'Microsoft.WindowsTerminal' 'msixbundle'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_user.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_user.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_user.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x64_user.InstallerSha256"
                }
            },
            "32bit": {
                "url": "$matchX86",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x86_user.InstallerSha256"
                }
            },
            "arm64": {
                "url": "$matchArm64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.arm64_user.InstallerSha256"
                }
            }
        }
    }
}
