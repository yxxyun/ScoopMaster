    "homepage": "https://github.com/morning-start/godot-engine-manager",
    "license": "MIT",
    "url": "https://github.com/morning-start/godot-engine-manager/releases/download/v1.5.0/gdem-x86_64-pc-windows-gnu.zip",
    "hash": "edc6b322e2caa6b3f884f4fe737a1931bc2b8df0e819f3c3b5d10cff9491a7a1",
    "bin": "gdem.exe",
    "checkver": "github",
    "autoupdate": {
        "url": "https://github.com/morning-start/godot-engine-manager/releases/download/v$version/gdem-x86_64-pc-windows-gnu.zip"
    },
    "env_set": {
        "GDEM_ROOT": "$persist_dir",
        "GODOT_PATH": "$dir/default/godot.exe"
