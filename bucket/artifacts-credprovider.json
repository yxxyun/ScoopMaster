{
    "version": "1.2.1",
    "description": "The Azure Artifacts Credential Provider enables dotnet, NuGet.exe, and MSBuild to interactively acquire credentials for Azure Artifacts feeds. ",
    "homepage": "https://github.com/microsoft/artifacts-credprovider",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/microsoft/artifacts-credprovider/releases/download/v1.2.1/Microsoft.NuGet.CredentialProvider.zip",
            "hash": "d7724a9ffacafe51e637793e9143af34881ed69daf0ff585258036cb7be27b71"
        }
    },
    "checkver": {
        "github": "https://github.com/microsoft/artifacts-credprovider"
    },
    "pre_install": "Copy-Item -Path $dir\\plugins\\netcore\\CredentialProvider.Microsoft -Destination $env:UserProfile\\.nuget\\plugins\\netcore\\CredentialProvider.Microsoft;Copy-Item -Path $dir\\plugins\\netfx\\CredentialProvider.Microsoft -Destination $env:UserProfile\\.nuget\\plugins\\netfx\\CredentialProvider.Microsoft",
    "pre_uninstall": "Remove-Item -Path $env:UserProfile\\.nuget\\plugins\\netcore\\CredentialProvider.Microsoft -Recurse;Remove-Item -Path $env:UserProfile\\.nuget\\plugins\\netfx\\CredentialProvider.Microsoft -Recurse",
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/microsoft/artifacts-credprovider/releases/download/v$version/Microsoft.NuGet.CredentialProvider.zip",
                "extract_dir": "dotnet-core-uninstall-$version"
            }
        }
    }
}
