    "hash": "19222e78770a41c3e26f227aaa8f24293a44d43d266dd4e59642be29fafb092f",
    "installer": {
        "script": [
            "$dest = \"$dir\\CertificateRevocation\"",
            "Expand-7zipArchive \"$dir\\$fname\" $dest -Removal",
