      "Spacedrive"
    ]
  ],
  "pre_install": [
    ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
    "A-Start-PreInstall"
  ],
  "post_install": [
    ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
    "A-Start-PostInstall",
    "$LinkPaths = @(\"$env:AppData\\spacedrive\")",
    "$LinkTargets = @(\"$persist_dir\\spacedrive\")",
    "A-New-LinkDirectory $LinkPaths $LinkTargets"
  ],
  "pre_uninstall": [
    "$bucket = $install.bucket",
    ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
    "A-Start-PreUninstall",
    "A-Stop-Process",
    "A-Remove-Link",
    "A-Remove-TempData @(\"$env:LocalAppData\\com.spacedrive.desktop\")"
  ],
  "post_uninstall": [
    "$bucket = $install.bucket",
    ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
    "A-Start-PostUninstall"
  ],
  "checkver": {
    "github": "https://github.com/spacedriveapp/spacedrive"
