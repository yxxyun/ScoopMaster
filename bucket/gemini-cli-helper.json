{
   "$schema": "https://raw.githubusercontent.com/lukesampson/scoop/master/schema.json",
   "version": "1.0.2",
   "description": "This helper implement script to switch profiles for Gemini CLI.",
   "suggest": {
      "Gemini CLI ": "silver886/gemini-cli"
   },
   "url": "https://raw.githubusercontent.com/silver886/scoop/gemini-cli-helper/v1.0.2/assets/gemini-cli-helper/Profile.ps1",
   "hash": "43a2177ca01312616072d03956f13f8618637f3ea3d3c20635f9ec2aeaa14eb4",
   "installer": {
      "script": [
         "$profileScript = \"$dir\\Profile.ps1\"",
         "$content = (Get-Content $profileScript).Replace('$dir', $dir).Replace('$persist_dir', $persist_dir).Replace('$scoopdir', $scoopdir)",
         "Set-Content -Force -Path $profileScript -Value $content -Encoding Ascii"
      ]
   },
   "bin": [
      [
         "Profile.ps1",
         "gemini-su"
      ]
   ],
   "persist": "profiles",
   "checkver": {
      "url": "https://api.github.com/repos/silver886/scoop/tags",
      "regex": "\"name\": *\"gemini-cli-helper/v([\\d.]+)\","
   },
   "autoupdate": {
      "url": "https://raw.githubusercontent.com/silver886/scoop/gemini-cli-helper/v$version/assets/gemini-cli-helper/Profile.ps1"
   }
}
