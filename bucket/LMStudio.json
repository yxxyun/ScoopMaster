            "hash": "45db561c2cd8fbd562da379b1981f8f09ad244ff9336c51ec8438bdd2c929a3d",
            "installer": {
                "script": [
                    "Remove-Item \"$dir\\*\" -Exclude \"app-64.7z\"",
