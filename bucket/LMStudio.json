                    "Remove-Item \"$dir\\app-64.7z\""
                ]
            }
        },
        "arm64": {
            "url": "https://installers.lmstudio.ai/win32/arm64/0.4.0-17/LM-Studio-0.4.0-17-arm64.exe#/dl.7z",
            "hash": "fa5135a2869c5391975e9d8c9d7e77a59419c398bd965181b65de2cbe83e6ac8",
            "installer": {
                "script": [
                    "Remove-Item \"$dir\\*\" -Exclude \"app-arm64.7z\"",
                    "Expand-7zipArchive \"$dir\\app-arm64.7z\" \"$dir\"",
                    "Remove-Item \"$dir\\app-arm64.7z\""
                ]
            }
        }
    },
    "extract_dir": "$PLUGINSDIR",
    "shortcuts": [
        [
            "LM Studio.exe",
            "LM Studio"
        ]
    ],
    "checkver": {
        "url": "https://lmstudio.ai/download",
        "regex": "\\\\\"version\\\\\":\\\\\"(?<ver>[\\d.]+)\\\\\",\\\\\"build\\\\\":\\\\\"(?<build>\\d+)\\\\\"",
        "replace": "${ver}-${build}"
    },
    "autoupdate": {
        "architecture": {
