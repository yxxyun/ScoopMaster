         "@(",
         "   'Set SHL = CreateObject(\"WScript.Shell\")'",
         "   \"call SHL.Run(`\"$env:LocalAppData\\Microsoft\\WindowsApps\\ms-teams.exe`\", o, false)\"",
         ") | Set-Content -Path \"$dir\\ms-teams.vbs\""
      ]
   },
   "uninstaller": {
      "script": [
