    "installer": {
        "script": [
            "if (!(is_admin)) { error \"$app requires admin rights to $cmd\"; break }",
            "Start-Process -Wait \"$dir\\filejuggler.exe\" -ArgumentList '/SILENT' -Verb RunAs | Out-Null",
            "Remove-Item \"$dir\\filejuggler.exe\""
        ]
    },
    "uninstaller": {
        "script": "if (!(is_admin)) { error \"$app requires admin rights to $cmd\"; break }"
    },
    "checkver": {
        "url": "https://www.filejuggler.com/news/",
        "regex": "File Juggler ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
