    "checkver": {
        "script": [
            "$workflow_runs = (Invoke-WebRequest 'https://api.github.com/repositories/903621663/actions/workflows/windows.yaml/runs?branch=master&status=success' -UseBasicParsing | ConvertFrom-Json).workflow_runs[0]",
