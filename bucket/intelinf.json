    "depends": "curl",
    "installer": {
        "script": "& \"$bucketsdir\\chris\\scripts\\$app\\$app.ps1\" -install"
    },
    "checkver": {
        "script": [
            "$html = curl -s 'https://www.intel.com/content/www/us/en/download/19347/chipset-inf-utility.html'",
            "$ret = [regex]::Matches($html, 'name=\"DownloadVersion\" content=\"(?<version>[\\d.]+)\"')",
            "$ver = $ret[0].groups['version'].value",
            "$ret = [regex]::Matches($html, 'download-button(.*)\"https://downloadmirror.intel.com/(?<url>.*exe)\"')",
            "$url = $ret[0].groups['url'].value",
            "[string]::Format('{0}|{1}', $ver, $url)"
        ],
        "regex": "(?<version>.+)\\|(?<url>.+)"
    },
    "autoupdate": {
        "url": "https://downloadmirror.intel.com/$matchUrl#/setup.exe"
    }
}
