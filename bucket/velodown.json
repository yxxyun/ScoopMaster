    "homepage": "https://github.com/mlm-games/velodown",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/mlm-games/velodown/releases/download/v0.2.12/velodown_0.2.12_x64-setup.exe",
