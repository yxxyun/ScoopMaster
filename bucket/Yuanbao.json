{
    "version": "2.25.0",
    "description": "腾讯元宝，腾讯的一款 AI 智能助手。An AI assistant by Tencent.",
    "homepage": "https://yuanbao.tencent.com/",
    "license": "Unknown",
    "architecture": {
        "64bit": {
            "url": "https://cdn-hybrid-prod.hunyuan.tencent.com/Desktop/official/bfe251e9c050a44dbdfb32e8b148fbda/yuanbao_2.25.0.618_x64.exe",
            "hash": "3645138146529a6043c5ae00c727dd9213fd5603a075c3dae765f11eed6bc97c"
        }
    },
    "shortcuts": [
        [
            "Yuanbao\\yuanbao.exe",
            "腾讯元宝"
        ]
    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "A-Require-Admin",
        "A-Install-Exe"
    ],
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall",
        "$LinkPaths = @(\"$env:AppData\\Tencent\\Yuanbao\")",
        "A-New-LinkDirectory $LinkPaths"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Require-Admin",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe -Uninstaller 'Yuanbao\\uninstall.exe'",
        "A-Remove-TempData @(\"$env:LocalAppData\\com.tencent.yuanbao\")"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "script": [
            ". \"$pwd\\bin\\utils.ps1\"",
            "$InstallerInfo = A-Get-InstallerInfoFromWinget 'Tencent.Yuanbao' 'exe'",
            "\"ver:$($InstallerInfo.PackageVersion);\"",
            "\"x64:$($InstallerInfo.x64_machine.InstallerUrl);\"",
            "\"x86:$($InstallerInfo.x86_machine.InstallerUrl);\"",
            "\"arm64:$($InstallerInfo.arm64_machine.InstallerUrl);\""
        ],
        "regex": "ver:(?<version>[^;]+)(?:; x64:(?<x64>[^;]*))?(?:; x86:(?<x86>[^;]*))?(?:; arm64:(?<arm64>[^;]*))?"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchX64",
                "hash": {
                    "url": "bin/scoop-auto-check-update-temp-data.jsonc",
                    "jsonpath": "$.x64_machine.InstallerSha256"
                }
            }
        }
    }
}
