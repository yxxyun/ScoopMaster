            "$houdini20 = \"$env:USERPROFILE\\Documents\\houdini20.5\"",
            "if (-Not (Test-Path $houdini20)) {",
            "    New-Item -ItemType Directory -Path $houdini20",
            "}",
            "New-DirectoryJunction -source \"$env:USERPROFILE\\Documents\\houdini21.0\" -target $houdini20"
        ]
    }
