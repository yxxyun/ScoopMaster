{
    "version": "0.0.1",
    "description": "chezmoi with personal settings",
    "homepage": "https://chezmoi.io",
    "license": "Freeware",
    "url": "https://github.com/excitoon/empty/releases/download/empty-1.0.7/empty-1.0.7.zip",
    "hash": "8739c76e681f900923b900c9df0ef75cf421d39cabb54650c4b9ad19b6a76d85",
    "depends": "main/chezmoi",
    "pre_install": [
        "if (!(is_admin)) { error \"$app requires admin rights to $cmd\"; break }"
    ],
    "post_install": [
        "chezmoi apply --source D:\\chezmoi --verbose --force --init",
        "chezmoi apply --source D:\\chezmoi\\scoop --destination D:\\scoop --verbose --force --init",
        "chezmoi apply --source D:\\chezmoi\\etc --destination C:\\Windows\\System32\\drivers\\etc --verbose --force --init"
    ],
    "uninstaller": {
        "script": ["scoop uninstall main/chezmoi"]
    }
}
