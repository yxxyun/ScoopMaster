        "  echo \"[General]\" >> \"$dir\\config_tray.ini\"",
        "  echo \"ShowFloatWnd=0\" >> \"$dir\\config_tray.ini\"",
        " }",
        "if (-not (Test-Path \"$persist_dir\\toolsv6.ini\")) { ",
        "  Copy-Item \"$dir\\tools\\toolsv6.ini\" \"$dir\\toolsv6.ini\" | Out-Null",
        "  (Get-Content \"$dir\\toolsv6.ini\").Replace('InstallPar=/VERYSILENT', 'InstallPar=/VERYSILENT /NOICONS') | Set-Content -Path \"$dir\\toolsv6.ini\"",
        " }"
    ],
    "post_install": [
