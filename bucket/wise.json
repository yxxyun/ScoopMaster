    "pre_install": [
        "if (-not (Test-Path \"$persist_dir\")) { ",
        "  New-Item -Path \"$persist_dir\" -ItemType Directory | Out-Null",
        "}",
        "if (-not (Test-Path \"$persist_dir\\config.ini\")) { ",
        "  New-Item -Force -Path \"$dir\\config.ini\" | Out-Null",
        "  echo \"[General]\" >> \"$dir\\config.ini\"",
        "  echo \"checkNewVer=3\" >> \"$dir\\config.ini\"",
        "  echo \"UpdateMode=3\" >> \"$dir\\config.ini\"",
        "  echo \"BootStartup=0\" >> \"$dir\\config.ini\"",
        "  echo \"StartBooster=0\" >> \"$dir\\config.ini\"",
        "  echo \"TurboCheck=0\" >> \"$dir\\config.ini\"",
        "  echo \"CloseTray=1\" >> \"$dir\\config.ini\"",
        "  echo \"ContextMenuFileShred=0\" >> \"$dir\\config.ini\"",
        "  echo \"MessageIDs=,684\" >> \"$dir\\config.ini\"",
        "  echo \"UserUE=1\" >> \"$dir\\config.ini\"",
        "  echo \"HintSpecialDay5=0\" >> \"$dir\\config.ini\"",
        "  echo \"[GUI]\" >> \"$dir\\config.ini\"",
        "  echo \"rmdlg=1\" >> \"$dir\\config.ini\"",
        " }",
        "if (-not (Test-Path \"$persist_dir\\config_tray.ini\")) { ",
