  "post_install": [
    "$pc98 = \"${scoopdir}\\persist\\machine-pc98\\pc98\\\"",
    "$pc98dosbin = \"${pc98}\\dosbin\\\"",
    "$null = New-Item -ItemType Directory -Force -Path $pc98dosbin",
    "Copy-Item -Force -Path \"${dir}\\*.com\" -Destination $pc98dosbin",
