{
    "version": "1.18.1b",
    "description": "Zen Browser Nonportable (works with 1Password integration)",
    "homepage": "https://www.zen-browser.app/",
    "license": "MPL-2.0",
    "url": "https://github.com/zen-browser/desktop/releases/download/1.18.1b/zen.installer.exe#/setup.exe",
    "hash": "fff49fa3c9fd35463c0838caccb8e5f571438ae313a5e90e192208db3ce6a0f6",
    "installer": {
        "file": "setup.exe",
        "args": "/S /DIR='C:\\program files\\zen browser'"
    },
    "bin": "C:\\program files\\zen browser\\zen.exe",
    "shortcuts": [
        ["C:\\program files\\zen browser\\zen.exe", "Zen Browser Profile Manager", "-P"]
    ],
    "pre_uninstall": "if (get-process -name zen -ea 0) {stop-process -name zen -force -ea 0}",
    "uninstaller": {
        "script": "start-process -wait 'C:\\program files\\zen browser\\uninstall\\helper.exe' -a '/S'",
    },
    "checkver": {
        "url": "https://github.com/zen-browser/desktop/tags",
        "regex": "releases/tag/([ab\\d.-]+)"
    },
    "autoupdate": {
        "url": "https://github.com/zen-browser/desktop/releases/download/$version/zen.installer.exe"
    }
}
