    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "A-Install-Exe -ArgumentList @('/S', \"/D=$dir\")"
    ],
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall",
        "$LinkPaths = @(\"$env:AppData\\Fishing Funds\")",
        "$LinkTargets = @(\"$persist_dir\\Fishing Funds\")",
        "A-New-LinkDirectory $LinkPaths $LinkTargets"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe -Uninstaller 'Uninstall Fishing Funds.exe' -ArgumentList @('/S')"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
