{
    "version": "1.8.10",
    "description": "强大的知识库，基于 Markdown 文件。Powerful knowledge base that works on top of a local folder of plain text Markdown files.",
    "homepage": "https://obsidian.md",
    "license": {
        "identifier": "Unknown",
        "url": "https://obsidian.md/eula"
    },
    "url": "https://github.com/obsidianmd/obsidian-releases/releases/download/v1.8.10/Obsidian-1.8.10.exe",
    "hash": "1fae2688513ca33a7b85eef53dc433e056487ac94c7fe95e5d33aa38b4cf7bd4",
    "bin": "obsidian.exe",
    "shortcuts": [
        [
            "obsidian.exe",
            "Obsidian"
        ]
    ],
    "pre_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreInstall",
        "A-Install-Exe"
    ],
    "post_install": [
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostInstall",
        "$LinkPaths = @(\"$env:AppData\\obsidian\")",
        "A-New-LinkDirectory $LinkPaths"
    ],
    "pre_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PreUninstall",
        "A-Stop-Process",
        "A-Remove-Link",
        "A-Uninstall-Exe -Uninstaller 'Uninstall Obsidian.exe'",
        "A-Remove-TempData @(\"$env:LocalAppData\\obsidian-updater\")"
    ],
    "post_uninstall": [
        "$bucket = $install.bucket",
        ". \"$bucketsdir\\$bucket\\bin\\utils.ps1\"",
        "A-Start-PostUninstall"
    ],
    "checkver": {
        "github": "https://github.com/obsidianmd/obsidian-releases"
    },
    "autoupdate": {
        "url": "https://github.com/obsidianmd/obsidian-releases/releases/download/v$version/Obsidian-$version.exe"
    }
}
